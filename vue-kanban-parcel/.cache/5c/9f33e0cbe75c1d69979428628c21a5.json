s: 100%;\n\n  background-color: $board-bg-color;\n  color: #eee;\n}\n\n.navbar {\n  padding-left: $gap;\n  display: flex;\n  align-items: center;\n  &.app {\n    background-color: $appbar-bg-color;\n    font-size: 1.5rem;\n  }\n  &.board {\n    font-size: 1.1rem;\n  }\n}\n\n.lists {\n  display: flex;\n  overflow-x: auto;\n  //width: 100%; // This is needed for FF < 54\n  > * {\n    flex: 0 0 auto; // 'rigid' lists\n    margin-left: $gap;\n  }\n  &::after {\n    content: '';\n    flex: 0 0 $gap;\n  }\n}\n\n.list {\n  width: $list-width;\n  height: calc(100% - #{$gap} - #{$scrollbar-thickness});\n\n  > * {\n    background-color: $list-bg-color;\n    color: #333;\n\n    padding: 0 $gap;\n  }\n\n  header {\n    line-height: $list-header-height;\n    font-size: 16px;\n    font-weight: bold;\n    border-top-left-radius: $list-border-radius;\n    border-top-right-radius: $list-border-radius;\n  }\n\n  footer {\n    line-height: $list-footer-height;\n    border-bottom-left-radius: $list-border-radius;\n    border-bottom-right-radius: $list-border-radius;\n    color: #888;\n  }\n\n  ul {\n    list-style: none;\n    margin: 0;\n\n    max-height: calc(100% - #{$list-header-height} - #{$list-footer-height});\n    overflow-y: auto;\n\n    li {\n      background-color: #fff;\n      padding: $gap;\n      &:not(:last-child) {\n        margin-bottom: $gap;\n      }\n\n      border-radius: $card-border-radius;\n      box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);\n\n      img {\n        display: block;\n        width: calc(100% + 2 * #{$gap});\n        margin: -$gap 0 $gap (-$gap);\n        border-top-left-radius: $card-border-radius;\n        border-top-right-radius: $card-border-radius;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"c9c2399750604a037b1e18467ed21bef","cacheData":{"env":{}}}