n                            <label class=\"label\">Password</label>\n                            <div class=\"control has-icons-left\">\n                              <input class=\"input is-link\" type=\"password\" placeholder=\"type your password here\" id=\"passwordRegister\" v-model=\"register.password\" required>\n                              <span class=\"icon is-small is-left\"><i class=\"fas fa-lock\"></i></span>\n                            </div>\n                        </div>\n                        <button type=\"submit\" class=\"button is-block is-primary is-fullwidth has-text-black\">Register</button>\n                    </form>\n                </div>\n                Already have an account? Login <a href=\"#\" @click.prevent=\"showLogin\">here</a>\n            </div>\n        </div>\n</template>\n\n<script>\nexport default {\n    name: 'RegisterForm',\n    data() {\n        return {\n            register: {\n                username: '',\n                email: '',\n                password: ''\n            },\n        }\n    },\n    props: ['currentPage'],\n    methods: {\n        createUser() {\n            axios({\n                method: 'POST',\n                url: `http://localhost:3000/register`,\n                data: {\n                    username: this.register.username,\n                    email: this.register.email,\n                    password: this.register.password\n                }\n            })\n            .then(user => {\n                this.$emit('changePage','login')\n            })\n            .catch(response => {\n                console.log(response)\n            })\n        },\n        showLogin() {\n            this.$emit('changePage', 'login')\n        }\n    }\n}\n</script>"},"lineCount":null}},"error":null,"hash":"39a981aa841c9d93da7e8e6c08287636","cacheData":{"env":{}}}