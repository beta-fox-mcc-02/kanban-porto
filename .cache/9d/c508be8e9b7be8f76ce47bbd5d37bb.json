rated":{"line":1,"column":590}}],"sources":{"src/components/AddTaskKanban.vue":"<template>\n    <div >\n        <form \n            id=\"addTask\" \n            class=\"form\" \n            @submit.prevent=\"addTask\"\n        >\n            <h3>ADD TASK</h3>\n            <div class=\"form-group\">\n                <label>Title</label><br>\n                <input type=\"text\" class=\"form-control\" v-model=\"newTask.title\">\n            </div>\n            <div class=\"form-group\">\n                <label>Description</label><br>\n                <input type=\"text\" class=\"form-control\" v-model=\"newTask.description\">\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </form>\n    </div>\n</template>\n\n<script>\n\nimport axios from 'axios'\n\nexport default {\n    name: \"AddTask\",\n    data() {\n        return {\n            newTask : {\n                title : \"\",\n                description: \"\"\n            }\n        }\n    },\n    methods: {\n        addTask(){\n            let newTask = {\n                title : this.newTask.title,\n                description : this.newTask.description\n            }\n\n            axios({\n                url: '/tasks',\n                method: 'post',\n                data: newTask,\n                headers: {\n                    access_token : localStorage.access_token\n                }\n            })\n                .then(task => {\n                    this.$emit('addedTask', 'login')\n                })\n                .catch(err => {\n                    this.$emit('error', 'cannot add task to the kanban')\n                })\n\n        }\n    }\n\n\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"a4bb1064c56952e7f0a6bee3787e5922","cacheData":{"env":{}}}