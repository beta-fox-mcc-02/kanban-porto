"column":0},"generated":{"line":90,"column":16}},{"source":"src/components/Category.vue","name":null,"original":{"line":84,"column":0},"generated":{"line":91,"column":0}},{"source":"src/components/Category.vue","name":null,"original":{"line":17,"column":0},"generated":{"line":92,"column":0}}],"sources":{"src/components/Category.vue":"<template>\n    <div class=\"col-3 mt-3\" style=\"background-color: rgba(255,255,255,0.4);\">\n        <h3 class=\"text-center mt-2 text-white\">{{category.name}}</h3>\n        <TaskCard \n                v-for=\"task in tasks\" \n                v-bind:key=\"task.id\"\n                v-bind:task=\"task\"\n                @editTask=\"editTask\"\n                @createTask=\"createTask\"\n                @forwardTask=\"forwardTask\"\n        ></TaskCard>\n    </div>\n</template>\n\n<script>\nimport TaskCard from './TaskCard';\nexport default {\n    name: \"Category\",\n    data() {\n        return {\n            tasks: []\n        }\n    },\n    props: {\n        category: Object\n    },\n    components: {\n        TaskCard\n    },\n    methods: {\n        getTask(){\n            axios({\n                method: \"get\",\n                url: \"http://localhost:3000/board/\" + this.category.id + \"/tasks\",\n                headers: {\n                    \"token\" : localStorage.token\n                }\n            })\n                .then(result => {\n                    console.log(result);\n                    \n                    this.tasks = result.data.data;\n                })\n                .catch(err => {\n                    console.log(err);\n                    \n                    this.$emit(\"error-message\",\n                    \"Email or password dismatch\");\n                })\n        },\n        editTask(id) {\n            this.$emit('editTask', id);\n        },\n        createTask() {\n            this.$emit('createTask');\n        },\n        forwardTask(id, category) {\n            axios({\n                method: \"patch\",\n                url: \"http://localhost:3000/board/\" + id + '/next',\n                headers: {\n                    \"token\" : localStorage.token\n                },\n                data: {\n                    CategoryId : category\n                }\n            })\n                .then(result => {\n                    // this.title = result.data.data.title;\n                    console.log(result);\n                    this.$emit('updateData');\n                })\n                .catch(err => {\n                    console.log(err);\n                    \n                    this.$emit(\"error-message\",\n                    \"Email or password dismatch\");\n                })\n        }\n    },\n    created() {\n        this.tasks = []\n        this.getTask();\n    }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"9f06f41cbbbca0ec8ef8e48448106eba","cacheData":{"env":{}}}